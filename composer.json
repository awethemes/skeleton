{
    "name": "awethemes/skeleton",
    "description": "A WordPress framework made for Awethemes projects.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "awethemes",
            "email": "support@awethemes.com"
        },
        {
            "name": "Nguyen Van Anh",
            "email": "anhskohbo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "pimple/pimple": "^3.2",
        "vlucas/valitron": "^1.4",
        "webdevstudios/cmb2": "^2.2"
    },
    "autoload": {
        "psr-4": { "Skeleton\\": "inc/" }
    },
    "scripts": {
        "post-install-cmd": [
            "Skeleton\\Support\\Composer_Scripts::clean"
        ],
        "post-update-cmd": [
            "Skeleton\\Support\\Composer_Scripts::clean"
        ],
        "clean": [
            "Skeleton\\Support\\Composer_Scripts::clean"
        ],
        "install-unit-tests": [
            "bash bin/install-wp-tests.sh wptest_skeleton root 'secret' localhost latest"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
